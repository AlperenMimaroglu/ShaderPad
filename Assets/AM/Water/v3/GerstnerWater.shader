				float3 ase_worldPos = mul(GetObjectToWorldMatrix(), v.vertex).xyz;
				float3 normalizeResult9_g17 = normalize( _WaveDirection1 );
				float3 Direction13_g17 = normalizeResult9_g17;
				float k8_g17 = ( TWO_PI / _WaveLength1 );
				float dotResult18_g17 = dot( ase_worldPos , ( Direction13_g17 * k8_g17 ) );
				float mulTime17_g17 = _TimeParameters.x * _WaveSpeed1;
				float temp_output_20_0_g17 = ( dotResult18_g17 - ( sqrt( ( k8_g17 * 9.8 ) ) * mulTime17_g17 ) );
				float temp_output_23_0_g17 = cos( temp_output_20_0_g17 );
				float Amp44_g17 = ( temp_output_55_0_g17 * k8_g17 );
				float temp_output_46_0_g17 = ( _WaveSteepness1 / Amp44_g17 );
				float temp_output_21_0_g17 = sin( temp_output_20_0_g17 );
				
				float2 break35_g17 = (( ( temp_output_21_0_g17 * Amp44_g17 ) * Direction13_g17 )).xz;
				float temp_output_30_0_g17 = ( Amp44_g17 * temp_output_23_0_g17 );
				float3 appendResult36_g17 = (float3(break35_g17.x , ( 1.0 - ( temp_output_46_0_g17 * temp_output_30_0_g17 ) ) , break35_g17.y));
				
				float3 objectToViewPos = TransformWorldToView(TransformObjectToWorld(v.vertex.xyz));
				float eyeDepth = -objectToViewPos.z;
				o.ase_texcoord8.x = eyeDepth;
				
				
				//setting value to unused interpolator channels and avoid initialization warnings
				o.ase_texcoord8.yzw = 0;
				#ifdef ASE_ABSOLUTE_VERTEX_POS
					float3 defaultVertexValue = v.vertex.xyz;
				#else
					float3 defaultVertexValue = float3(0, 0, 0);
				#endif
				float3 vertexValue = ( ( temp_output_55_0_g17 * ( temp_output_23_0_g17 * float3(0,1,0) ) ) - ( ( ( temp_output_55_0_g17 * temp_output_46_0_g17 ) * temp_output_21_0_g17 ) * Direction13_g17 ) );
				#ifdef ASE_ABSOLUTE_VERTEX_POS
					v.vertex.xyz = vertexValue;
				#else
					v.vertex.xyz += vertexValue;
				#endif
				v.ase_normal = appendResult36_g17;

				float3 positionWS = TransformObjectToWorld( v.vertex.xyz );
				float3 positionVS = TransformWorldToView( positionWS );
				float4 positionCS = TransformWorldToHClip( positionWS );

				VertexNormalInputs normalInput = GetVertexNormalInputs( v.ase_normal, v.ase_tangent );

